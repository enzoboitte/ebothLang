{
  "comments": {
    "lineComment": "#"
  },
  "brackets": [
    ["(", ")"],
    ["[", "]"]
  ],
  "autoClosingPairs": [
    { "open": "\"", "close": "\"", "notIn": ["string"] },
    { "open": "(", "close": ")" },
    { "open": "[", "close": "]" }
  ],
  "surroundingPairs": [
    ["\"", "\""],
    ["(", ")"],
    ["[", "]"]
  ],
  "wordPattern": "[a-zA-Z_][a-zA-Z0-9_]*|[+\\-*/%]|\\d+",
  "indentationRules": {
    "increaseIndentPattern": "^\\s*(proc|const)\\s+\\w+.*\\bin\\b\\s*$",
    "decreaseIndentPattern": "^\\s*end\\s*$"
  },
  "folding": {
    "markers": {
      "start": "^\\s*(proc|const)\\s+",
      "end": "^\\s*end\\s*$"
    }
  }
}
